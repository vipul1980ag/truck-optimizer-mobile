{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "cache": {
        "clear": true
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://truck-capacity-optimizer.onrender.com"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_URL": "https://truck-capacity-optimizer.onrender.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
